{
  "name": "cemkarabulut.com",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "fmt": "pnpm rome check . --apply-unsafe && pnpm rome format . --write",
    "analyze": "ANALYZE=true next build",
    "lighthouse": "lighthouse https://cemkarabulut.com --output=html --output-path=./lighthouse-report.html",
